{
  "name": "pure-js",
  "type": "module",
  "scripts": {
    "start": "bun run --bun server/index.js",
    "dev": "concurrently --kill-others 'bun build:css --watch' 'bun start --watch'",
    "build:css": "bun run --bun build.js",
    "build:css:prod": "bun run build:css && cleancss -o static/style.css static/style.css"
  },
  "dependencies": {
    "hono": "latest"
  },
  "devDependencies": {
    "@unocss/cli": "latest",
    "clean-css-cli": "latest",
    "concurrently": "latest",
    "unocss": "latest"
  }
}
